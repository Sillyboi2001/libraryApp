{
  "name": "server",
  "version": "1.0.0",
  "description": "library app API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "babel ../src -d build --ignore node_modules",
    "dev": "nodemon --exec babel-node src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --coverage --testTimeout=10000 --forceExit",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "startdev": "nodemon --experimental-modules --es-module-specifier-resolution=node index.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node index.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@jest/globals": "^27.5.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.27.1",
    "codecov": "^3.8.3",
    "cors": "^2.8.5",
    "datauri": "^4.1.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "router": "^1.3.5",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-arrow-functions": "^7.16.7",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2"
  }
}
